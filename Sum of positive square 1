import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner abc = new Scanner(System.in);
        int n = abc.nextInt();
        
        int sum = 0;
        for (int i = 0; i < n; i++) {
            
            int num = abc.nextInt();
            if (num > 0) {
                sum += num * num; 
            }
        }
        System.out.println(sum);
        
    }
}
