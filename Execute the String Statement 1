import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class GreetUser {
    public static void main(String[] args) {
        
        Scanner scanner = new Scanner(System.in);

        
 
        String userName = scanner.nextLine();

        
        System.out.printf("Hai " + userName + "!");
        System.out.printf("    Welcome to Programming Language...");

        
        scanner.close();
    }
}
